{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["lambda:invokeFunction"],
      "Resource": [
        "arn:aws:lambda:ap-northeast-1:${account_id}:function:${func_name}",
        "arn:aws:lambda:ap-northeast-1:${account_id}:function:${func_name}:*"
      ]
    }
  ]
}
