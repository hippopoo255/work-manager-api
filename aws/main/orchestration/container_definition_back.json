[
  {
    "name": "web",
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],

    "image": "015211996394.dkr.ecr.ap-northeast-1.amazonaws.com/${ecr_image_prefix}/web:latest",
    "cpu": 8,
    "memoryReservation": 128,
    "essential": true,
    "links": ["app:app"],
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "${log_groups.0}",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "${log_groups.0}"
      }
    }
  },
